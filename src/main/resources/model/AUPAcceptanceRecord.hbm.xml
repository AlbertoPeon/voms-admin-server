<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.glite.security.voms.admin.model.AUPAcceptanceRecord" table="aup_acc_record">	
		
		<id name="id" type="long">
			<generator class="native"/>
		</id>
	
        <natural-id>
            <many-to-one 
              name="aupVersion"
              class="org.glite.security.voms.admin.model.AUPVersion"
              column="aup_version_id"
              not-null="true"/>
            
            <many-to-one 
              name="user"
              class="org.glite.security.voms.admin.model.VOMSUser"
              column="usr_id"
              not-null="true"/>
        
        </natural-id>
		
        <property 
          name="lastAcceptanceDate"
          column="last_acceptance_date"
          type="timestamp"
          not-null="true"/>
		
	</class>

</hibernate-mapping>